#include <iostream>
using namespace std;
int main()
{
	double n0;
	double n1;
	double n2;
	double ave;

	cout << "If you want to some real magic, enter 3 numbers!" << endl;
	cin >> n0;
	cin >> n1;
	cin >> n2;

	ave = (n0 + n1 + n2) / 3;

	cout << "There is your perfect number!" << ave << endl;
	return 0;

}
