#include <iostream>
using namespace std;
int main()
{
	double score1;
	double score2;
	double score3;
	double avPerformance;

	cout << "Enter Scores from 3 last matches:"<< endl;
	cin >> score1;
	cin >> score2;
	cin >> score3;

	avPerformance = (score1 + score2 + score3) / 3;

	cout << "Your Average Performance is:" << avPerformance;




	return 0;
}
